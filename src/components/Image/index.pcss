.imageContainer {
  width: 128px;
  height: 128px;
  background-position: center;
  background-repeat: no-repeat;
  composes: accent-lightest from "../../styles/colors.pcss";
  composes: m-border-radius from "../../styles/borders.pcss";
  display: grid;
  align-items: center;
  justify-items: center;
}

.image {
  width: 100%;
  height: 100%;
  composes: m-border-radius from "../../styles/borders.pcss";
  object-fit: cover;
}
